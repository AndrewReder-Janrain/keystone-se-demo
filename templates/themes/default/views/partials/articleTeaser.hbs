<div class="thumbnail">
  <div class="carousel slide">
    <div class="carousel-inner">
      <img class="img-responsive" src={{this.image.url}} alt={{this.title}}>
      <div class="carousel-caption" style="padding-bottom: 0px;">
        <p>{{this.title}}</p>
        <p><a href="/article/{{this.slug}}" class="btn btn-primary" role="button">Read More</a></p>
      </div>
    </div>
  </div>
</div>
